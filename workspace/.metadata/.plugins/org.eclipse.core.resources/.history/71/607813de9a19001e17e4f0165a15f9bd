program test302

const int jedan = 1;

int i;
bool ibool;

int niz2[];

int mat[][];


{
	
		
	
	void main()
	{
		mat=new int[2][2];
		mat[0][0]=1;
		mat[0][1]=1;
		mat[1][0]=1;
		mat[1][1]=1;
		
	
		niz2 = mat.map(curr => curr * 3);
		i = 0;
		while (i < 3){
			print(niz2[i]);
			i++;
		}
		
	}
}
