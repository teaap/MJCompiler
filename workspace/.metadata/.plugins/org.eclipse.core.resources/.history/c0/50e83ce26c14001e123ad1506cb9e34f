//Test 3

program test3
char curr;

{
//provera map
//tacno je ako ispise sve 2
	
	void main() char niz[],niz1[];{
		niz=new char[10];
		niz[0]='a';
		niz[1]='b';
		niz[2]='c';
		niz[3]='d';
		niz[4]='e';
		niz1=niz.map(curr=>curr);
		print(niz1[0]);
		print(niz1[1]);
		print(niz1[2]);
		print(niz1[3]);
		print(niz1[4]);
	}
}

